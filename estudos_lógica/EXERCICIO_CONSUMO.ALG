Algoritmo "exercicio_consumo"


Var

   distancia, consumo, media: real


Inicio

      escreva("Digite a distância percorrida(KM's): ")
      leia(distancia)
      
      escreva("Qual foi o consumo dessa distância: ")
      leia(consumo)
      
      media <- distancia / consumo
      
      escreva("Média de Consumo = ", media:5:3)

      // Conclusão do Exercício - Consumo

Fimalgoritmo